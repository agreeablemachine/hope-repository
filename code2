function adjustFeedbackBox(width, height) {
    // The width and heights are percentages. 
    let styles = `
    .overlay_window {
        height: ${height}%;
        width: ${width}%;
        top: ${(100-height)/2}%;
        left: ${(100-width)/2}%;
    }
    `
    var styleSheet = document.createElement("style")
    styleSheet.textContent = styles
    document.head.appendChild(styleSheet)
}

adjustFeedbackBox(57,69);



