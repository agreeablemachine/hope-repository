fullscreenButton.style.position = 'absolute';
fullscreenButton.style.top = '10px';
fullscreenButton.style.left = '10px';
fullscreenButton.style.backgroundColor = '#2d89ef';
fullscreenButton.style.color = '#fff';
fullscreenButton.style.border = 'none';
fullscreenButton.style.padding = '0.25em 0.5em';
fullscreenButton.style.borderRadius = '4px';
fullscreenButton.style.cursor = 'pointer';
fullscreenButton.onclick = function () {
  if (!document.fullscreenElement) {
    hqDiv.requestFullscreen();
  } else {
    document.exitFullscreen();
  }
};

// Close Button
let closeButton = document.createElement('button');
closeButton.textContent = 'X';
closeButton.style.backgroundColor = '#ff5c5c';
closeButton.style.color = '#fff';
closeButton.style.border = 'none';
closeButton.style.padding = '0.25em 0.5em';
closeButton.style.cursor = 'pointer';
closeButton.style.position = 'absolute';
closeButton.style.top = '10px';
closeButton.style.right = '10px';
closeButton.onclick = function () {
  // Close all sections when close button is clicked
  const sections = ['homeContent', 'abroadContent', 'legacyContent', 'oppositionTabs'];
  sections.forEach(id => {
    const section = document.getElementById(id);
    section.style.display = 'none';
  });

  // Optionally remove content or parent div
  hqDiv.remove();
};

