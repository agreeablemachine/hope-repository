function updateHeadlineWithImage(headlineHref, newHeadline, imageUrl) {
    if (!cnnContent) {
        console.error("cnnContent is not initialized.");
        return;
    }

    // Find the <a> tag with the specific href
    const headlineLink = cnnContent.querySelector(`a[href="${headlineHref}"]`);

    if (headlineLink) {
        const headlineElement = headlineLink.closest('li'); // Get the parent <li> element
        const headlineImage = headlineElement.querySelector('img');

        // Update headline text and image
        headlineLink.textContent = newHeadline;
        headlineImage.src = imageUrl;
        headlineImage.alt = newHeadline;
        console.log(`Updated headline with href ${headlineHref} with new text and image.`);
    } else {
        console.log(`Headline with href ${headlineHref} not found.`);
    }
}
