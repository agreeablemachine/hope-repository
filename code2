        document.getElementById("houseMajorityText").innerText = "House Majority: Republican";

        document.getElementById("senateImage").src = "https://i.imgur.com/6Kzu6De.png"; // Replace with actual URL
        document.getElementById("senateMajorityText").innerText = "Senate Majority: Republican";

        // Leaders
        document.getElementById("houseLeaderImage").src = "https://i.imgur.com/zy0Htu0.png"; // Replace with actual URL
        document.getElementById("houseLeaderText").innerText = "Speaker of the House: Newt Gingrich";

        document.getElementById("senateLeaderImage").src = "https://i.imgur.com/5nL1HEB.png"; // Replace with actual URL
        document.getElementById("senateLeaderText").innerText = "Senate Majority Leader: Bob Dole";
        updateCandidateDescription('Bob Dole', 'Bob Dole is the current Senate Majority Leader, and a long-time U.S. Senator from Kansas, having served since 1969. After his run with President Gerald Ford in 1976 marked a pivitol moment in his career, Dole aims to finish the job he started and become president in 1996. Given the lack of notable party members running, Dole is very likely to be nominated.');
    } else if (MassiveGOPMajority== 1) {
        // Republican Congress
        // Majority Images and Texts
        document.getElementById("houseImage").src = "https://i.imgur.com/CcgTevZ.png"; // Replace with actual URL
        document.getElementById("houseMajorityText").innerText = "House Majority: Republican";

        document.getElementById("senateImage").src = "https://i.imgur.com/TzWBSFC.png"; // Replace with actual URL
        document.getElementById("senateMajorityText").innerText = "Senate Majority: Republican";

        // Leaders
        document.getElementById("houseLeaderImage").src = "https://i.imgur.com/zy0Htu0.png"; // Replace with actual URL
        document.getElementById("houseLeaderText").innerText = "Speaker of the House: Newt Gingrich";

        document.getElementById("senateLeaderImage").src = "https://i.imgur.com/5nL1HEB.png"; // Replace with actual URL
        document.getElementById("senateLeaderText").innerText = "Senate Majority Leader: Bob Dole";
        updateCandidateDescription('Bob Dole', 'Bob Dole is the current Senate Majority Leader, and a long-time U.S. Senator from Kansas, having served since 1969. After his run with President Gerald Ford in 1976 marked a pivitol moment in his career, Dole aims to finish the job he started and become president in 1996. Given the lack of notable party members running, Dole is very likely to be nominated.');
    } else {
        // Republican Congress
        // Majority Images and Texts
        document.getElementById("houseImage").src = "https://i.imgur.com/hhN5EAQ.png"; // Replace with actual URL
        document.getElementById("houseMajorityText").innerText = "House Majority: Republican";

        document.getElementById("senateImage").src = "https://i.imgur.com/Eif6mNc.png"; // Replace with actual URL
        document.getElementById("senateMajorityText").innerText = "Senate Majority: Republican";

        // Leaders
        document.getElementById("houseLeaderImage").src = "https://i.imgur.com/zy0Htu0.png"; // Replace with actual URL
        document.getElementById("houseLeaderText").innerText = "Speaker of the House: Newt Gingrich";

        document.getElementById("senateLeaderImage").src = "https://i.imgur.com/5nL1HEB.png"; // Replace with actual URL
        document.getElementById("senateLeaderText").innerText = "Senate Majority Leader: Bob Dole";
        updateCandidateDescription('Bob Dole', 'Bob Dole is the current Senate Majority Leader, and a long-time U.S. Senator from Kansas, having served since 1969. After his run with President Gerald Ford in 1976 marked a pivitol moment in his career, Dole aims to finish the job he started and become president in 1996. Given the lack of notable party members running, Dole is very likely to be nominated.');
    }
    };
   // Function to update dynamic content for "International Standing"
function updateInternationalStanding() {
    const standingText = document.getElementById('dynamicStandingText');

    // Example logic for International Standing text
    if (FPWin == 5) {
        standingText.textContent = "Strong alliances and global influence.";
    } else if (FPWin >= 2 && FPWin <= 4) {
        standingText.textContent = "Moderate influence abroad, working on key partnerships.";
    } else if (FPWin == 1) {
        standingText.textContent = "Emerging influence in some regions.";
    } else {
        standingText.textContent = "Limited influence on the global stage.";
    }
}

// Function to toggle section display
function toggleSection(sectionId) {
    const sections = ['homeContent', 'abroadContent', 'legacyContent', 'oppositionTabs'];
    sections.forEach(id => {
        const section = document.getElementById(id);
        if (id === sectionId) {
            section.style.display = section.style.display === 'block' ? 'none' : 'block';
        } else {
            section.style.display = 'none';
        }
    });
    document.getElementById('contentText').style.display = 'none';

    // Update Legacy, Ideology, and International Standing when respective sections are opened
    if (sectionId === 'legacyContent') {
        updateLegacyAndIdeology();
    } else if (sectionId === 'abroadContent') {
        updateInternationalStanding();  // Update the international standing when "The Fight Abroad" is clicked
    } else if (sectionId === 'homeContent') {
        updateApprovalRating()
    }
}
    
        // Event listeners for section links
        hqContent.querySelector('#home').addEventListener('click', function(event) {
            event.preventDefault();
            toggleSection('homeContent');
        });
    
        hqContent.querySelector('#abroad').addEventListener('click', function(event) {
            event.preventDefault();
            toggleSection('abroadContent');
        });
    
        hqContent.querySelector('#opposition').addEventListener('click', function(event) {
            event.preventDefault();
            toggleSection('oppositionTabs');
        });
    
        hqContent.querySelector('#legacy').addEventListener('click', function(event) {
            event.preventDefault();
            toggleSection('legacyContent');
        });
    
        // Function to load candidate content dynamically
