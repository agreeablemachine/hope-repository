        document.getElementById("senateLeaderImage").src = "https://i.imgur.com/Jk2Kg8L.png";
        document.getElementById("senateLeaderText").innerText = "Senate Majority Leader: George Mitchell";
    } else if (SlimGOPCongress == 1) {
