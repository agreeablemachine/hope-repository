  candImg = $(".person_image")[0];
  if (candImg) {
    candImg.src = e.image;
	console.log("TRYING");
    $("#final_results_description")[0].style = `
      text-align:left;
      width: 72%;
	  height:71%;
      display: block;
      margin-left: auto;
      margin-right: auto;
	  overflow: auto;
      color: #000000
    `
	 console.log("RESULTS UPDATED");
  }		
}, 10)

return html;
}

