        // Candidate data with pros, cons, and strategies
const candidates = [
    {
        name: 'Bob Dole',
        image: 'https://i.imgur.com/5nL1HEB.png',
        description: 'Bob Dole is a former Senate Majority Leader, and a long-time U.S. Senator from Kansas, having served since 1969. After his run with President Gerald Ford in 1976 marked a pivitol moment in his career, Dole aims to finish the job he started and become president in 1996. Given the lack of notable party members running, Dole is very likely to be nominated.',
        pros: 'Highly experienced in Washington, and well-respected among almost all factions of the Republicans. Will do well with the old and rural vote.',
        cons: '73 years old, would be oldest president inagurated. Almost 0 appeal to young voters. 3X loser, with loses in 1976, 1980, and 1988.',
        strategy: 'Emphasize your youth, tie Dole to the establishment and unpopular Republicans.'
    }
];
    // Define updateOppositionTabs function first
function updateOppositionTabs(candidate) {
    const candidateImage = document.getElementById('candidateImage');
    const candidateDescription = document.getElementById('candidateDescription');
    const candidatePros = document.getElementById('candidatePros');
    const candidateCons = document.getElementById('candidateCons');
    const candidateStrategy = document.getElementById('candidateStrategy');

    // Update content dynamically
    candidateImage.src = candidate.image;
    candidateDescription.textContent = candidate.description;

    // Update Pros
    candidatePros.innerHTML = '';
    candidate.pros.split('. ').forEach(pro => {
        if (pro.trim()) {
            const li = document.createElement('li');
            li.textContent = pro.trim();
            candidatePros.appendChild(li);
        }
    });

    // Update Cons
    candidateCons.innerHTML = '';
    candidate.cons.split('. ').forEach(con => {
        if (con.trim()) {
            const li = document.createElement('li');
            li.textContent = con.trim();
            candidateCons.appendChild(li);
        }
    });

    // Update Strategy
    candidateStrategy.textContent = candidate.strategy;
}

function updateCandidateDescription(candidateName, newDescription) {
    // Find the candidate in the array
    const candidate = candidates.find(c => c.name === candidateName);
    if (!candidate) {
        console.error('Candidate not found!');
        return;
    }

    // Update the candidate's description
    candidate.description = newDescription;

    // Update the DOM to reflect the new description
    updateOppositionTabs(candidate);
}

        // Populate the candidate tabs dynamically
        const tabsContainer = hqContent.querySelector('#tabs');
        candidates.forEach(candidate => {
            const tabButton = document.createElement('button');
            tabButton.textContent = candidate.name;
            tabButton.style.padding = '10px';
            tabButton.style.margin = '5px';
            tabButton.style.backgroundColor = '#ffd700';
            tabButton.style.border = 'none';
            tabButton.style.cursor = 'pointer';
            tabButton.addEventListener('click', function() {
                loadCandidateContent(candidate);
            });
            tabsContainer.appendChild(tabButton);
        });

            // Function to dynamically update Legacy and Ideology content
    function updateLegacyAndIdeology() {
        const legacyText = document.getElementById('legacyText');
        const ideologyText = document.getElementById('ideologyText');

        // Legacy content
        if (Legacy == 7) {
            legacyText.textContent = "Perfect";
        } else if (Legacy >= 4 && Legacy <= 6) {
            legacyText.textContent = "Remarkable";
        } else if (Legacy == 3) {
            legacyText.textContent = "Decent";
        } else if (Legacy >= 1 && Legacy >= 2) {
            legacyText.textContent = "Developing";
        } else {
            legacyText.textContent = "Minimal";
        }

        // Ensure Ideology is a number
Ideology = Number(Ideology);

// Debugging log to see the value of Ideology
console.log("Ideology value: ", Ideology);

// Ideology content
if (Ideology === 0) {
    ideologyText.textContent = "Moderate";
} else if (Ideology >= -5 && Ideology <= -1) {
    ideologyText.textContent = "Third Way";
} else if (Ideology >= -10 && Ideology <= -6) {
    ideologyText.textContent = "Blue Dog";
} else if (Ideology >= 1 && Ideology <= 5) {
    ideologyText.textContent = "New Deal Democrat";
} else if (Ideology >= 6 && Ideology <= 13) {
    ideologyText.textContent = "Progressive";
} else {
    ideologyText.textContent = "Undefined";
}
    }

    function updateApprovalRating() {
        // Update dynamic approval text based on LEGWin variable
    const approvalText = document.getElementById('approvalText');
        if (LEGWin == 7) {
            approvalText.textContent = "Excellent";
        } else if (LEGWin >= 3 && LEGWin <= 6) {
            approvalText.textContent = "Moderate";
        } else if (LEGWin >= 1 && LEGWin <= 2) {
            approvalText.textContent = "Bad";
        } else {
            approvalText.textContent = "Abysmal";
        }

        // Update the page dynamically based on DemCongress value
    // Update the page dynamically based on DemCongress value
    if (DemCongress === 1) {
        // Democratic Congress
        // Majority Images and Texts
        document.getElementById("houseImage").src = "https://i.imgur.com/WunYh6Q.png";
